<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/nutricoop_icon_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/nutricoop_icon_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.NutriCoop"
        tools:targetApi="31">
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/title_activity_main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.formularios.formularioPaciente.FormularioPacienteActivity"/>
        <activity android:name=".ui.formularios.formularioClinica.FormularioClinicaActivity"/>
        <activity android:name=".ui.detail.detailPaciente.PacienteDescriptionActivity"/>
        <activity android:name=".ui.detail.detailClinica.ClinicaDescriptionActivity"/>

    </application>

</manifest>